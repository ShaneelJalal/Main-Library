# Prime number checker between 1 - 10000
def prime_checker(number):

	is_prime = True
	for i in range(2, number):
		if number % i == 0:
			is_prime = False
	
	if is_prime:
		print(f"{number} is a prime number")
	else:
		print(f"{number} is not a prime number")
		
#Run
n = int(input("Enter a number: ")) # checks this number
prime_checker(number = n)
